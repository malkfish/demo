{
  "name": "demo",
  "version": "1.0.0",
  "description": "spotify web api demo",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js",
    "stop": "taskkill -f -im node.exe",
    "client-install": "npm install --prefix client",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.3",
    "concurrently": "^9.0.1",
    "express": "^4.21.0",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.7"
  }
}
